<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
          integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
    <link rel="stylesheet" href="/bootstrap.css">
    <link rel="stylesheet" href="/style.css">
    <link rel="icon"
          href="data:image/png;base64,">
    <title>YelpCamp</title>
</head>
<body>
<nav class="navbar navbar-expand-md navbar-dark">
    <a href="/" class="navbar navbar-brand"><img class="navbarIcon"
                                                 src="data:image/png;base64,"
                                                 alt="">YELPCAMP</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav">
            <li class="nav-item ">
                <a class="nav-link navbarLink" href="/campgrounds">Home</a>
            </li>
            <li class="nav-item">
                <a class="nav-link  navbarLink" href="/campgrounds/new">add campground</a>
            </li>


            <span class="auth" >

                <% if(!currentUser){ %>

                    <li class="nav-item login">
                <a class="nav-link  navbarLink" href="/login">login</a>
            </li>
                    <li class="nav-item register ">
                <a class="nav-link  navbarLink" href="/register">register</a>
            </li>

                <% }else{ %>
                    <li class="nav-item" style="width: 200px; display: flex; align-items: center"><a class="nav-link navbarLink"
                                                                                                    href="#"> <img
                                    src="https://i.pinimg.com/originals/0c/3b/3a/0c3b3adb1a7530892e55ef36d3be6cb8.png"
                                    alt=""
                                    style="width: 15px;height: 15px; margin-right: 5px;"><%= currentUser.username %></a>
            </li>
                    <li class="nav-item register ">
                <a class="nav-link  navbarLink" href="/logout">Logout</a>
            </li>
                <% } %>
            </span>
        </ul>
    </div>
</nav>
<% if (error && error.length > 0){ %>
    <div class="container">
        <div class="alert alert-danger">
            <%= error %>
        </div>
    </div>
<% } %>
<% if (success && success.length > 0){ %>
    <div class="container">
        <div class="alert alert-success">
            <%= success %>
        </div>
    </div>

<% } %>
